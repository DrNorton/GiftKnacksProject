<!DOCTYPE html>
<html data-ng-app="giftknacksApp" ng-cloak class="ng-cloak">
<head>
    <base href="/">
    <title ng-bind="title">@ViewBag.Title</title>
    <meta name="description" content="{{metaDescription}}">
    <meta name="keywords" content="{{metaKeywords}}">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="fragment" content="!">
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    @Styles.Render("~/Content/css")

</head>
<body>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
        ga('create', 'UA-70826077-1', 'auto');
    </script>
    @RenderBody()


    @Scripts.Render("~/bundles/vendors")
    <script src="http://giftknackapi.azurewebsites.net/signalr/hubs"></script>
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false&language=en-US"></script>

</body>
</html>
